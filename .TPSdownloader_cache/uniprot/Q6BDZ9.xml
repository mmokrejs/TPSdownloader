    <comment type="function">
      <text evidence="2">Catalyzes the conversion of syn-copalyl diphosphate to the phytoalexin precursor stemarene.</text>
    </comment>
    <comment type="catalytic activity">
      <reaction>
        <text>9alpha-copalyl diphosphate = diphosphate + stemar-13-ene</text>
        <dbReference id="RHEA:25552" type="Rhea"/>
        <dbReference id="CHEBI:33019" type="ChEBI"/>
        <dbReference id="CHEBI:50069" type="ChEBI"/>
        <dbReference id="CHEBI:58622" type="ChEBI"/>
        <dbReference id="4.2.3.33" type="EC"/>
      </reaction>
    </comment>
    <comment type="cofactor">
      <cofactor evidence="1">
        <name>Mg(2+)</name>
        <dbReference id="CHEBI:18420" type="ChEBI"/>
      </cofactor>
      <text evidence="1">Binds 3 Mg(2+) ions per subunit.</text>
    </comment>
    <comment type="alternative products">
      <event type="alternative splicing"/>
      <isoform>
        <id>Q6BDZ9-1</id>
        <name>1</name>
        <sequence type="displayed"/>
      </isoform>
